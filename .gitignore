# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history

.idea/*

/client/dist/
/client/locales/*
!/client/locales/.keep
/client/node_modules/
/client/yarn-error.log

/config/database.yml

/coverage/

/public/js/
/solr/
/accounting_adapter/
/optigo/
