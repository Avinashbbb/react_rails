  "private": true,
  "scripts": {
    "build:production": "yarn clean && yarn locales:production && yarn build:production:client && yarn build:production:server",
    "build:production:client": "NODE_ENV=production webpack -p --config webpack/client.config.js",
    "build:production:server": "NODE_ENV=production webpack --config webpack/server.config.js",
    "build:development": "yarn clean && yarn locales & yarn build:development:server & yarn build:development:client",
    "build:development:client": "NODE_ENV=development webpack -w --config webpack/client.config.js",
    "build:development:server": "NODE_ENV=development webpack -w --config webpack/server.config.js",
    "build:test": "yarn clean && yarn build:test:client && yarn build:test:server",
    "build:test:client": "NODE_ENV=test webpack --config webpack/client.config.js",
    "build:test:server": "NODE_ENV=test webpack --config webpack/server.config.js",
    "clean": "rm -rf ../public/js/*",
    "eslint": "eslint .",
    "locales": "yaml-to-intl '../config/locales/**/*.yml' -o './locales/index.js' -w",
    "locales:development": "yaml-to-intl '../config/locales/**/*.yml' -o './locales/index.js'",
    "locales:production": "yaml-to-intl '../config/locales/**/*.yml' -o './locales/index.js'"
  },
