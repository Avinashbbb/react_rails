    "lodash": "4.17.10",
    "material-ui-pickers": "2.0.4",
    "moment": "2.22.2",
    "optigo-redux": "git+ssh://git@github.com/groupefungo/optigo-redux.git#franchise",
    "prop-types": "15.6.2",
    "rails-fetch": "git+ssh://git@github.com/groupefungo/rails-fetch.git",
